{
  "kind": "collectionType",
  "collectionName": "products",
  "info": {
    "singularName": "product",
    "pluralName": "products",
    "displayName": "product"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "produktImg": {
      "type": "media",
      "multiple": true,
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ]
    },
    "produktTitle": {
      "type": "string",
      "required": true
    },
    "produktPrice": {
      "type": "decimal",
      "required": true
    },
    "produktDescription": {
      "type": "text",
      "required": true
    },
    "category": {
      "type": "enumeration",
      "enum": [
        "başörtüsü",
        "Namaz kıyafetleri"
      ]
    }
  }
}
